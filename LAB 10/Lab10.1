<mxfile host="app.diagrams.net">
  <diagram name="Palindrome Flowchart" id="PalindromeFlow">
    <mxGraphModel dx="1500" dy="900" grid="1" gridSize="10" guides="1" tooltips="1" connect="1" arrows="1" fold="1" page="1" pageScale="1" pageWidth="827" pageHeight="1169">
      <root>
        <mxCell id="0"/>
        <mxCell id="1" parent="0"/>
        
        <!-- Start -->
        <mxCell id="start" value="Start" style="ellipse;whiteSpace=wrap;html=1;aspect=fixed;" vertex="1" parent="1">
          <mxGeometry x="320" y="20" width="120" height="60" as="geometry"/>
        </mxCell>
        
        <!-- กำหนดตัวแปร -->
        <mxCell id="proc1" value="กำหนดตัวแปร word[100], pass = 1" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="250" y="100" width="300" height="60" as="geometry"/>
        </mxCell>
        
        <!-- แสดงข้อความกรอกคำ -->
        <mxCell id="io1" value="แสดงข้อความ 'กรุณากรอกคำ:'" style="parallelogram;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="250" y="180" width="300" height="60" as="geometry"/>
        </mxCell>
        
        <!-- รับค่า word -->
        <mxCell id="io2" value="รับค่า word" style="parallelogram;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="250" y="260" width="300" height="60" as="geometry"/>
        </mxCell>
        
        <!-- คำนวณ len -->
        <mxCell id="proc2" value="len = strlen(word)" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="250" y="340" width="300" height="60" as="geometry"/>
        </mxCell>
        
        <!-- กำหนด i = 0 -->
        <mxCell id="proc3" value="i = 0" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="250" y="420" width="300" height="60" as="geometry"/>
        </mxCell>
        
        <!-- Condition i < len/2 -->
        <mxCell id="cond1" value="i &lt; len / 2 ?" style="rhombus;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="250" y="500" width="300" height="90" as="geometry"/>
        </mxCell>
        
        <!-- Condition tolower(word[i]) != tolower(word[len-1-i]) -->
        <mxCell id="cond2" value="tolower(word[i]) != tolower(word[len-1-i]) ?" style="rhombus;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="250" y="610" width="300" height="90" as="geometry"/>
        </mxCell>
        
        <!-- Process pass = 0 -->
        <mxCell id="proc4" value="pass = 0" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="250" y="730" width="300" height="60" as="geometry"/>
        </mxCell>
        
        <!-- Process i = i + 1 -->
        <mxCell id="proc5" value="i = i + 1" style="rounded=0;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="600" y="730" width="300" height="60" as="geometry"/>
        </mxCell>
        
        <!-- Condition pass == 1 -->
        <mxCell id="cond3" value="pass == 1 ?" style="rhombus;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="250" y="850" width="300" height="90" as="geometry"/>
        </mxCell>
        
        <!-- Output Pass -->
        <mxCell id="out1" value="แสดง 'Pass.'" style="parallelogram;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="50" y="970" width="300" height="60" as="geometry"/>
        </mxCell>
        
        <!-- Output Not Pass -->
        <mxCell id="out2" value="แสดง 'Not Pass.'" style="parallelogram;whiteSpace=wrap;html=1;" vertex="1" parent="1">
          <mxGeometry x="550" y="970" width="300" height="60" as="geometry"/>
        </mxCell>
        
        <!-- Stop -->
        <mxCell id="stop" value="Stop" style="ellipse;whiteSpace=wrap;html=1;aspect=fixed;" vertex="1" parent="1">
          <mxGeometry x="320" y="1070" width="120" height="60" as="geometry"/>
        </mxCell>

        <!-- Connectors -->
        <mxCell id="edge1" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="start" target="proc1"/>
        <mxCell id="edge2" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="proc1" target="io1"/>
        <mxCell id="edge3" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="io1" target="io2"/>
        <mxCell id="edge4" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="io2" target="proc2"/>
        <mxCell id="edge5" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="proc2" target="proc3"/>
        <mxCell id="edge6" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="proc3" target="cond1"/>
        <mxCell id="edge7" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="cond1" target="cond2"/>
        <mxCell id="edge8" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="cond2" target="proc4" value="True"/>
        <mxCell id="edge9" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="cond2" target="proc5" value="False"/>
        <mxCell id="edge10" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="proc5" target="cond1"/>
        <mxCell id="edge11" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="proc4" target="cond3"/>
        <mxCell id="edge12" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="cond1" target="cond3" value="False"/>
        <mxCell id="edge13" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="cond3" target="out1" value="True"/>
        <mxCell id="edge14" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="cond3" target="out2" value="False"/>
        <mxCell id="edge15" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="out1" target="stop"/>
        <mxCell id="edge16" style="edgeStyle=orthogonalEdgeStyle;rounded=0;orthogonalLoop=1;jettySize=auto;html=1;endArrow=classic;" edge="1" parent="1" source="out2" target="stop"/>
      </root>
    </mxGraphModel>
  </diagram>
</mxfile>
